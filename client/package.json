  "description": "The frontend client for Rustwrench (https://github.com/nozzlegear/rustwrench)",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "watch": "concurrently --raw \"npm run client\" \"npm run nancy\"",
    "client": "webpack-dev-server --inline --hot --port 7002",
    "client:asp": "webpack-dev-server --inline --hot --port 7002 --aspNet",
    "nancy": "call \"../Rustwrench.Hosting.Self/bin/Debug/Rustwrench.Hosting.Self.exe\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
